package pages;

import org.openqa.selenium.By;

public class LoginPage {

	public static By linkSignIn       = By.linkText("Sign in");
	public static By txtEmail         = By.id("email");
	public static By txtPassword       = By.id("passwd");
	public static By btSubmit         = By.id("SubmitLogin");
	public static By txtSearch = By.id("search_query_top");
	public static By btSearch = By.name("submit_search");
	
	
	
}
